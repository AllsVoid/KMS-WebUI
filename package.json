{
  "name": "kms-tools",
  "private": true,
  "version": "1.2.0",
  "description": "一个用于生成kms激活脚本的小工具",
  "author": "一纸忘忧 <i@ikxin.com>",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "service": "bun --watch service/index.ts",
    "tsc": "vue-tsc",
    "db:clear": "rm -r service/migrations sqlite.db",
    "db:init": "drizzle-kit generate --name init_db",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio --verbose",
    "kill-vlmcsd": "pkill -f vlmcsd"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.55.2",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/static": "^1.0.3",
    "@vueuse/core": "^10.10.0",
    "@vueuse/integrations": "^10.10.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "elysia": "^1.0.22",
    "less": "^4.2.0",
    "markdown-it-shiki": "^0.9.0",
    "pinia": "^2.1.7",
    "unocss": "^0.60.3",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/bun": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "better-sqlite3": "^10.0.0",
    "cron": "^3.1.7",
    "drizzle-kit": "^0.21.4",
    "drizzle-orm": "^0.30.10",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "^0.8.8",
    "vite": "^5.2.11",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.19"
  }
}
